{
	"name": "monastyr",
	"private": true,
	"version": "0.0.0",
	"scripts": {
		"b": "cd backend && npm run",
		"w": "cd webapp && npm run",
		"dev": "concurrently \"npm run b dev\" \"npm run w dev\"",
		"dev-build": "npm run b build && npm run dev",
		"types": "concurrently \"npm run b types\" \"npm run w types\"",
		"lint": "concurrently \"npm run b lint\" \"npm run w lint\"",
		"prettify": "concurrently \"npm run b prettify\" \"npm run w prettify\""
	},
	"devDependencies": {
		"@types/express": "4.17.21",
		"@types/jsonwebtoken": "^9.0.9",
		"concurrently": "^9.1.2",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-config-react-app": "^7.0.1",
		"eslint-config-standard-with-typescript": "^43.0.1",
		"prettier": "^3.5.3",
		"ts-node": "^10.9.1",
		"ts-node-dev": "^2.0.0"
	},
	"dependencies": {
		"@ckeditor/ckeditor5-build-classic": "^44.3.0",
		"@ckeditor/ckeditor5-react": "^9.5.0",
		"@prisma/client": "^6.7.0",
		"@types/dompurify": "^3.2.0",
		"@types/react-select": "^5.0.1",
		"dompurify": "^3.2.5",
		"express": "^4.21.2",
		"jsonwebtoken": "^9.0.2",
		"prisma": "^6.7.0",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"react-select": "^5.10.1",
		"suneditor": "^2.47.5",
		"suneditor-react": "^3.6.1"
	}
}
